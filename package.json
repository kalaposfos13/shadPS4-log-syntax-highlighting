{
  "name": "shad-log-syntax-highlighting",
  "displayName": "shadPS4 log syntax highlighting",
  "description": "Syntax highlighting and coloring for ShadPS4 log files.",
  "publisher": "kalaposfos",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalaposfos13/shadPS4-log-syntax-highlighting"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "compile": "tsc -p ./"
  },
  "contributes": {
    "languages": [
      {
        "id": "shadlog",
        "aliases": [
          "shad log",
          "Shad Log"
        ],
        "extensions": [
          ".temp.log"
        ],
        "filenamePatterns": [
          "shad*.txt",
          "CUSA*.log",
          "BREW*.log"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "shadlog",
        "scopeName": "source.shadlog",
        "path": "./syntaxes/shadlog.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/vscode": "^1.101.0"
  },
  "dependencies": {
    "typescript": "^5.8.3"
  }
}
