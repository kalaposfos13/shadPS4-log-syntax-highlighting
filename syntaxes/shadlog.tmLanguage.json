{
  "name": "shad log",
  "scopeName": "source.shadlog",
  "patterns": [
    {
      "name": "shadlog.fileline",
      "match": "\\s\\S+\\.\\S+:\\S+\\s"
    },
    {
      "name": "shadlog.library",
      "match": "^\\[\\S+\\]"
    },
    {
      "name": "shadlog.level.trace",
      "match": "<Trace>"
    },
    {
      "name": "shadlog.level.debug",
      "match": "<Debug>"
    },
    {
      "name": "shadlog.level.info",
      "match": "<Info>"
    },
    {
      "name": "shadlog.level.warning",
      "match": "<Warning>"
    },
    {
      "name": "shadlog.level.error",
      "match": "<Error>"
    },
    {
      "name": "shadlog.level.critical",
      "match": "<Critical>"
    }
  ],
  "repository": {}
}
